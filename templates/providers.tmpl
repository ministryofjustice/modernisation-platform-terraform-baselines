## This file is automatically generated based on what regions are enabled in an AWS account.

%{ for region in account_regions ~}
provider "aws" {
  region = "${region}"
  alias  = "${baseline_provider_key}-${region}"
  %{ if baseline_assume_role }
  assume_role {
    role_arn = var.baseline_assume_role
  }
  %{ endif }
}

%{ endfor }
